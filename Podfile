use_frameworks!
inhibit_all_warnings!

platform :ios, '10.0'

def shared_pods
    pod 'AsyncDisplayKit'
    pod 'RxSwift', '~> 3.0'
    pod 'Alamofire', '~> 4.4'
    pod 'Gloss'
    pod 'JHSpinner'
    pod 'Charts'
    pod 'RealmSwift'
    pod 'ESTabBarController-swift'
end

target 'pr' do
    shared_pods
end

target 'urlaction' do
    shared_pods
end
